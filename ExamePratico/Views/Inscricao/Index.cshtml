@model IEnumerable<ExamePratico.Models.Inscricao>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Template.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Participante)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Evento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DataInscricao)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstadoDoEvento)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Participante)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Evento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DataInscricao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EstadoDoEvento)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.InscricaoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.InscricaoID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.InscricaoID })
        </td>
    </tr>
}

</table>
